    <joint name="torso_to_abduct_fr_j" type="continuous">
        <axis xyz="1 0 0"/>
        <origin rpy="0 0 0" xyz="0.212 -0.051 0.0"/>
        <parent link="body"/>
        <child link="abduct_fr"/>
        <limit effort="18" lower="-6.28" upper="6.28" velocity="25"/>
        <dynamics damping="0.01" friction="0.2"/>
    </joint>

