        <visual>
            <geometry>
                <!--mesh filename="meshes/bp_abad_fr.obj"/-->
                <mesh filename="meshes/bp_abad2.dae"/>
            </geometry>
            <origin rpy="3.1415926 0 0" xyz="-0.063 0.0 0.0"/>
        </visual>
    </link>

